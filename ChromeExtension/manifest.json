{
    "manifest_version": 3,
    "name": "FocusGuard",
    "version": "0.1.0",
    "permissions": [
      "history",
      "storage",
      "tabs",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }